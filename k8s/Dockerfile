FROM centos
RUN yum install -y wget
RUN wget https://dl.k8s.io/v1.18.6/kubernetes.tar.gz
RUN wget https://dl.k8s.io/v1.18.6/kubernetes-client-linux-amd64.tar.gz
RUN wget https://dl.k8s.io/v1.18.6/kubernetes-server-linux-amd64.tar.gz
RUN wget https://dl.k8s.io/v1.18.6/kubernetes-node-linux-amd64.tar.gz